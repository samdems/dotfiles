    <script nonce="58XNHbBA7mE4h6ProrQE9g==">// When making any changes to this function be sure to update the same function used inside the actual app.
function recordWorkingSetSize(name) {
    if (window.electronSafeIpc &&
        window.desktop &&
        window.desktop.perf &&
        window.desktop.perf.getProcessMemoryInfo) {
        window.electronSafeIpc.send('telemetryEvent', {
            name: name,
            Perf_WorkingSetSizeKB: window.desktop.perf.getProcessMemoryInfo().workingSetSize
        });
    }
}
function getPerformanceTimestamp() {
    if (performance && performance.now) {
        return performance.now();
    }
    return Date.now();
}
function send(method, url, token) {
    return new Promise(function (success, failure) {
        var xmlhttp = new XMLHttpRequest();
        xmlhttp.responseType = 'json';
        xmlhttp.onreadystatechange = function () {
            if (xmlhttp.readyState == XMLHttpRequest.DONE) {
                if (xmlhttp.status == 200) {
                    success(xmlhttp.response);
                }
                else {
                    failure({ error: { status: xmlhttp.status, statusText: xmlhttp.statusText } });
                }
            }
        };
        xmlhttp.open(method, url, true);
        if (token) {
            xmlhttp.setRequestHeader('Authorization', 'Bearer ' + token);
        }
        xmlhttp.send();
    });
}
function storeReason(reason) {
    // store reason for not making start up call to CSA
    window['startupCallSkipped'] = reason;
}
function recordStartupCallPerformance(callStartTime, syncToken) {
    if (syncToken) {
        window['startup_update_duration'] = getPerformanceTimestamp() - callStartTime;
        window['startupCallSyncToken'] = true;
    }
    else {
        window['startup_clean_duration'] = getPerformanceTimestamp() - callStartTime;
    }
}
function getAccessTokenFromCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return undefined;
}
function parseJwt(token) {
    var base64 = token.split('.')[1];
    return JSON.parse(window.atob(base64));
}
function getAccessToken(resource, tokenPrefix) {
    var offset = 120;
    var ssoToken = undefined; // of type SsoToken
    // Getting token from cookie
    var accessToken = getAccessTokenFromCookie("chatSvcAggCookie");
    // check the token validity
    if (accessToken) {
        var parsedToken = parseJwt(accessToken);
        // If expiration is within offset, it will force renew
        if (parsedToken && parsedToken.exp
            && (parsedToken.exp > Math.round(new Date().getTime() / 1000.0) + offset)) {
            ssoToken = { "expiration": parsedToken.exp, "token": accessToken, "aud": parsedToken.aud };
            // update local storage
            localStorage.setItem(tokenPrefix + resource, JSON.stringify(ssoToken));
        }
    } // if(accessToken)
    // do we not have the sso token still? Look in local storage
    if (!ssoToken) {
        var localToken = localStorage.getItem(tokenPrefix + resource);
        if (localToken) {
            try {
                var localTokenObj = JSON.parse(localToken);
                // if we parsed the token in local storage and it is valid
                if (localTokenObj && localTokenObj.expiration
                    && (localTokenObj.expiration > Math.round(new Date().getTime() / 1000.0) + offset)) {
                    ssoToken = localTokenObj;
                }
                else {
                    storeReason('LSTokenExpired');
                }
            }
            catch (e) {
                storeReason('LSTokenParseError');
            }
        }
        else {
            storeReason('LSTokenNotAvailable');
        } // if(localToken)
    } // if(!ssoToken)
    return ssoToken && ssoToken.token;
}
function prefetchFunction() {
    // Send off an ipc having first thing to report memory consumption and other data about app resources
    recordWorkingSetSize('memoryIndexHeadStart');
    var startupData;
    var tokenPrefix = 'ts.sso';
    /* Settings will be filled in by build and desktop client*/
    var enableStartupAPICall = window.enableStartupApi;
    var chatSvcAggHost = 'https://chatsvcagg.teams.microsoft.com';
    var chatSvcAggResource = 'https://chatsvcagg.teams.microsoft.com';
    if (!enableStartupAPICall) {
        storeReason('EnableStartupOff');
    }
    if (enableStartupAPICall && chatSvcAggHost && chatSvcAggResource) {
        var accessToken = getAccessToken(chatSvcAggResource, tokenPrefix);
        if (accessToken) {
            var accessTokenObj = parseJwt(accessToken);
            var oid = accessTokenObj && accessTokenObj.oid;
            // get the sync token
            var syncToken = localStorage.getItem('ts.' + oid + '.priv_convlist_syncstate');
            var resourcePath = '/api/v1/teams/users/me';
            if (syncToken) {
                resourcePath = '/api/v1/teams/users/me/updates?syncToken=' + encodeURIComponent(syncToken);
            }
            var callStart = getPerformanceTimestamp();
            startupData = send('GET', chatSvcAggHost + resourcePath, accessToken).then(function (data) {
                recordStartupCallPerformance(callStart, syncToken);
                return data;
            }, function (error) {
                recordStartupCallPerformance(callStart, syncToken);
                return Promise.reject(error);
            });
        }
    }
}
prefetchFunction();
//# sourceMappingURL=prefetch-html.js.map</script>

    <script nonce="58XNHbBA7mE4h6ProrQE9g==">
      var waitForAppCache = false;
      var timeoutHandle = null;
      var appStateUpdater = null;
      var isSurfaceHub = !!(window &&
        window.Microsoft &&
        window.Microsoft.PPISkype &&
        window.Microsoft.PPISkype.Authentication);

      if (window &&
        window.Microsoft &&
        window.Microsoft.Teams &&
        window.Microsoft.Teams.Hwa &&
        window.Microsoft.Teams.Hwa.Common &&
        window.Microsoft.Teams.Hwa.Common.ShellServiceProvider &&
        window.Microsoft.Teams.Hwa.Common.ShellServiceProvider.instance &&
        window.Microsoft.Teams.Hwa.Common.ShellServiceProvider.instance.resolve &&
        window.Microsoft.Teams.Hwa.Common.Constants &&
        window.Microsoft.Teams.Hwa.Common.Constants.Components &&
        window.Microsoft.Teams.Hwa.Common.Constants.Components.appStateUpdater) {
          let serviceProvider =  window.Microsoft.Teams.Hwa.Common.ShellServiceProvider.instance;
          if (serviceProvider) {
              let service = serviceProvider.resolve(window.Microsoft.Teams.Hwa.Common.Constants.Components.appStateUpdater);
              if (service && service.updateWebAppState) {
                  appStateUpdater = service;
              }
          }
      }

      function sendAppStateChanged(state) {
        if (window.electronSafeIpc) {
          window.electronSafeIpc.send('appStateChanged', state);
        }

        // Send Index page updates to HWA Desktop client
        if ((appStateUpdater != null) && !isSurfaceHub) {
          appStateUpdater.updateWebAppState(state);
        }

      }

      sendAppStateChanged('IndexInitialize');

      //check for app cache
      var appCache = this.window.applicationCache;
      if (appCache) {
        waitForAppCache = true;

        //new cache downloaded. refresh now
        function handleUpdate() {
          console.log('index.html: cache updated. refresh');
          sendAppStateChanged('IndexAppCacheUpdateReady');
          removeListeners();
          window.location.reload();
        }

        //error updating cache. log error
        function handleError(error) {
          console.error('index.html: error getting app cache: ' + error);
          sendAppStateChanged('IndexAppCacheError');
          removeListeners();
        }

        //no update available. do nothing
        function handleNoUpdate() {
          console.log('index.html: cache up-to-date');
          sendAppStateChanged('IndexAppCacheNoUpdate');
          removeListeners();
        }

        function removeListeners() {
          if (waitForAppCache) {
            console.log('index.html: remove startup cache listeners');
            appCache.removeEventListener('error', handleError);
            appCache.removeEventListener('noupdate', handleNoUpdate);
            appCache.removeEventListener('updateready', handleUpdate);
            waitForAppCache = false;
            timeoutHandle && clearTimeout(timeoutHandle);
            timeoutHandle = null;
          }
        }

        //register for event regardless
        console.log('index.html: register for events');
        appCache.addEventListener('updateready', handleUpdate, false);
        appCache.addEventListener('error', handleError, false);
        appCache.addEventListener('noupdate', handleNoUpdate, false);

        //check
        console.log('index.html: check for appcache update. State: ' + appCache.status);
        if (appCache.status == appCache.UPDATEREADY) {
          console.log('index.html: update found. refresh page.');
          sendAppStateChanged('IndexAppCacheUpdateReady');
          handleUpdate();
        } else if (appCache.status == appCache.IDLE) {
          console.log('index.html: there is no update. resume app start');
          sendAppStateChanged('IndexAppCacheIdle');
          removeListeners();
        } else if (appCache.status !== appCache.UNCACHED) {
          console.log('index.html: update not ready, continue with app start');
          sendAppStateChanged('IndexAppCacheUpdateNotReady');
          timeoutHandle = window.setTimeout(function(){
            removeListeners();
          }, 2000);
        }
      }
    </script>

