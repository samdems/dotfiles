    <script nonce="58XNHbBA7mE4h6ProrQE9g==">
      /** When we upload compiled files to CDN, we upload them in different folder in different env.
        For example, In Prod, we upload all compiled -minified files in /hashedjs folder,
        In INT env, we upload them in /st-statics-df/hashedjs folder whereas in PR build,
        we upload directly in the root directory. So, For lazy loading, the webpack public path should be
        different for different env.
        There's a webpack runtime public path plugin using which we can set different public paths for different env.
        Please check - https://www.npmjs.com/package/runtime-public-path-webpack-plugin */
      window.webpack_runtime_public_path = 'https://statics.teams.microsoft.com/hashedjs/';
      window.lazy_ng1_scripts_root_path = 'https://statics.teams.microsoft.com/hashedjs/';

      var rootUri = "https://teams.microsoft.com/";

      function sendAppDiag(area) {
        var url = rootUri +  "appdiag/" + area;
        var httpRequest = new XMLHttpRequest();
        try {
          httpRequest.open("GET", url, true);
          httpRequest.send(null);
        } catch (e) {
          console.log(e);
        }
        console.log("appdiag telemetry sent");
      }

      window.appStartTimer = window.setTimeout(function() {
        console.log("appstart failure was detected");
        const allowedRestartCount = 2;
        const appStartFailureCountString = "appStartFailureCount";
        sendAppDiag("appstartfailure");
        if (window.localStorage) {
          var appStartFailureCount = window.localStorage.getItem(appStartFailureCountString);
          var count = (appStartFailureCount) ? parseInt(appStartFailureCount) + 1 : 0; // if appStartFailureCount is not truthy set to zero

          if (count < allowedRestartCount) {
            // only restart twice
            window.localStorage.setItem(appStartFailureCountString, count);
            window.location.reload();
          } else {
            // if already restarted twice, redirect to oops page
            window.localStorage.removeItem(appStartFailureCountString);
            window.location.replace(rootUri + "error/oops");
          }
        } else {
          // if localStorage is not available redirect to oops page
          window.location.replace(rootUri + "error/oops");
        }
      }, 120000);
    </script>

