    <script nonce="58XNHbBA7mE4h6ProrQE9g==">
      var themedStyleSheets = [];
       themedStyleSheets.push('https://statics.teams.microsoft.com/hashedcss/stylesheets.theme-contrast-rtl.min-2af3709b.css');
           themedStyleSheets.push('https://statics.teams.microsoft.com/hashedcss/stylesheets.theme-contrast.min-acf56649.css');
           themedStyleSheets.push('https://statics.teams.microsoft.com/hashedcss/stylesheets.theme-dark-rtl.min-0ebdd180.css');
           themedStyleSheets.push('https://statics.teams.microsoft.com/hashedcss/stylesheets.theme-dark.min-9008d441.css');
           themedStyleSheets.push('https://statics.teams.microsoft.com/hashedcss/stylesheets.theme-default-rtl.min-e2bc15c6.css');
           themedStyleSheets.push('https://statics.teams.microsoft.com/hashedcss/stylesheets.theme-default.min-08146a15.css');
          

      function readCookie(name) {
        var nameEq = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
          var c = ca[i];
          while (c.charAt(0)==' ') c = c.substring(1,c.length);
          if (c.indexOf(nameEq) == 0) return c.substring(nameEq.length,c.length);
        }
        return null;
      }

      function setCssLanguageDirection() {
        // This is used to select the stylesheet because we do not have the language direction from localizationService soon enough
        let rtlCodes = ['ar', 'he', 'fl'];
        if (window.TS_PREFERRED_LOCALES &&
            window.TS_PREFERRED_LOCALES[0] &&
            typeof window.TS_PREFERRED_LOCALES[0] == "string" &&
            rtlCodes.indexOf(window.TS_PREFERRED_LOCALES[0].split('-')[0]) != -1) {
          return 'rtl';
        }
        return 'ltr';
      }

      function getStylesheetUrl(theme) {
        for (var i = 0; i < themedStyleSheets.length; i++) {
          var themedStyleSheet = themedStyleSheets[i];
          if (themedStyleSheet.indexOf('.theme-' + theme + '.') !== -1) {
            return themedStyleSheet;
          }
        }
        return themedStyleSheets[0];
      }

      function getStylesheetUrl2(theme) {
        let direction = setCssLanguageDirection();
        let themeWithDir = direction === 'rtl' ? (theme + '-' + direction) : theme;
        for (var i = 0; i < themedStyleSheets.length; i++) {
          if (themedStyleSheets[i].indexOf('.theme-' + themeWithDir + '.') != -1) {
            return themedStyleSheets[i];
          }
        }
        return themedStyleSheets[themedStyleSheets.length -1];
      }

      function setStylesheetFromCookie(fnDone, appInitialize) {
        var htmlHeadElement = document.head || document.getElementsByTagName('head')[0];
        var themeTransitionStyle = document.createElement('style');
        themeTransitionStyle.innerHTML = '* { transition-property: background-color; transition-duration: .22s; transition-timing-function: ease-out; }';
        if (!appInitialize) {
          htmlHeadElement.appendChild(themeTransitionStyle);
        }

        var previous = document.getElementById("themed-stylesheet");

        var theme = readCookie("storedTheme") || "default";
        var cssUrl = getStylesheetUrl2(theme);

        var link = document.createElement("link");
        link.id = "themed-stylesheet";
        link.rel = "stylesheet";
        link.href = cssUrl;

        console.log('Loading stylesheet: ' + cssUrl);
        var fnDoneWrapper = function() {
          if (fnDone) {
            fnDone();
          }

          var loadingStylesheet = document.getElementById('loading-stylesheet');
          if (loadingStylesheet) {
            htmlHeadElement.removeChild(loadingStylesheet);
          }

          if (themeTransitionStyle.parentElement == htmlHeadElement) {
            htmlHeadElement.removeChild(themeTransitionStyle);
          }

          htmlHeadElement = null;
          themeTransitionStyle = null;
          previous = null;
        }

        link.onload = function () {
          console.log('Loaded stylesheet: ' + cssUrl);
          if (previous && previous.parentElement) {
            previous.parentElement.removeChild(previous);
          }
          fnDoneWrapper();
        }

        link.onerror = function() {
          console.log('Failed to load stylesheet: ' + cssUrl);
          fnDoneWrapper();
        }

        htmlHeadElement.appendChild(link);
        link = null;
      }

      window.setStylesheetFromCookie = setStylesheetFromCookie;

      setStylesheetFromCookie(function () {
        _.defer(function() {
          $('body').addClass('initialized');
        });
      }, true);

      $(window).load(function loadApp() {
        angular.bootstrap(document.documentElement, ['teamspace']);
        if (window.electronSafeIpc &&
          window.desktop &&
          window.desktop.perf &&
          window.desktop.perf.getProcessMemoryInfo) {
          window.electronSafeIpc.send('telemetryEvent', {
            name: 'memoryIndexAppBootstrap',
            Perf_WorkingSetSizeKB: window.desktop.perf.getProcessMemoryInfo().workingSetSize
          });
        }
      });

      sendAppStateChanged('IndexJsLoaded');
    </script>
